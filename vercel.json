{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "headers": [
    {
      "source": "/admin/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/payload-jobs/run?limit=20",
      "schedule": "*/30 * * * *"
    }
  ],
  "git": {
    "deploymentEnabled": false
  }
}
